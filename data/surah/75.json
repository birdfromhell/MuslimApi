        "tr": "bal<u>aa</u> q<u>aa</u>diriina 'al<u>aa</u> an nusawwiya ban<u>aa</u>nah<strong>u</strong>"
    },
    {
        "ar": "\u0628\u064e\u0644\u0652 \u064a\u064f\u0631\u0650\u064a\u062f\u064f \u0627\u0644\u0652\u0625\u0650\u0646\u0652\u0633\u064e\u0627\u0646\u064f \u0644\u0650\u064a\u064e\u0641\u0652\u062c\u064f\u0631\u064e \u0623\u064e\u0645\u064e\u0627\u0645\u064e\u0647\u064f",
