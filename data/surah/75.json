    },
    {
        "ar": "\u0625\u0650\u0644\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u0650\u0647\u064e\u0627 \u0646\u064e\u0627\u0638\u0650\u0631\u064e\u0629\u064c",
