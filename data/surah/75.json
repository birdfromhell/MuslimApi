        "ar": "\u0648\u064e\u0648\u064f\u062c\u064f\u0648\u0647\u064c \u064a\u064e\u0648\u0652\u0645\u064e\u0626\u0650\u0630\u064d \u0628\u064e\u0627\u0633\u0650\u0631\u064e\u0629\u064c",
