    },
    {
        "ar": "\u062b\u064f\u0645\u0651\u064e \u0623\u064e\u0648\u0652\u0644\u064e\u0649\u0670 \u0644\u064e\u0643\u064e \u0641\u064e\u0623\u064e\u0648\u0652\u0644\u064e\u0649\u0670",
        "id": "kemudian kecelakaanlah bagimu (hai orang kafir) dan kecelakaanlah bagimu.",
        "nomor": "35",
        "tr": "tsumma awl<u>aa</u> laka fa-awl<u>aa</u>"
