        "nomor": "20",
        "tr": "kall<u>aa</u> bal tu<u>h</u>ibbuuna <strong>a</strong>l'<u>aa</u>jila<strong>ta</strong>"
    },
    {
        "ar": "\u0648\u064e\u062a\u064e\u0630\u064e\u0631\u064f\u0648\u0646\u064e \u0627\u0644\u0652\u0622\u062e\u0650\u0631\u064e\u0629\u064e",
