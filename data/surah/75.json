    },
    {
        "ar": "\u0648\u064e\u0644\u064e\u0627 \u0623\u064f\u0642\u0652\u0633\u0650\u0645\u064f \u0628\u0650\u0627\u0644\u0646\u0651\u064e\u0641\u0652\u0633\u0650 \u0627\u0644\u0644\u0651\u064e\u0648\u0651\u064e\u0627\u0645\u064e\u0629\u0650",
