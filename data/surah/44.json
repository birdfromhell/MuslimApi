    },
    {
        "ar": "\u0648\u064e\u0627\u0644\u0652\u0643\u0650\u062a\u064e\u0627\u0628\u0650 \u0627\u0644\u0652\u0645\u064f\u0628\u0650\u064a\u0646\u0650",
        "id": "Demi Kitab (Al Qur'an) yang menjelaskan,",
        "nomor": "2",
